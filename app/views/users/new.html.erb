<%= @user.errors.full_messages %>
<%= form_for @user do |f| %>
	<p>
	<%= f.label :username %>
	<%= f.text_field :name %>
	</p>
	<p>
	<%= f.label :password %>
	<%= f.password_field :password %>
	</p>
	<p>
	<%= f.label :e_mail, 'E-mail' %>
	<%= f.text_field :email %>
	</p>
	<%= f.submit :register%>
<% end %>
